<header>
  <h1>Overflow</h1>
  <p>
    <a href="#questions">new</a>
    <a href="#">comments</a>
    <a href="#">popular</a>
    <a href="#">login</a>
  </p>
</header>


<div class='question-container'>
  <% @questions.each do |question| %>

    <article id="<%= question.id %>">
      <form method="post" action='/questions/<%= question.id %>/vote' class="inline">
        <button type="submit" name="submit_param" value="submit_value" class="upvote-button"></button>
      </form>
      <h2><a href='/questions/<%= question.id %>'><%= question.title %></a></h2>
      <p>
        <span class='votes'><%= question.votes %></span>
        <span class='username'><%= question.username %></span>
        <span class='timestamp'><%= question.time_since_creation %></span>
        <span class='comment-count'><%= question.comment_count %></span>
      </p> 
    </article>

  <% end %>
</div>

<form id="questions" method="post" action="/questions">
  <input type="text" name="titleeeeee" placeholder="title">
  <input type="submit" value="submit new post">
</form>